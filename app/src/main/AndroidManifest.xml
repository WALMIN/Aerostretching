<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.aerostretching.booking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminActivity" />
        <activity android:name=".BookTrainingActivity" />
        <activity android:name=".ContactsActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".MessagesActivity" />
        <activity android:name=".MyPageActivity" />
        <activity android:name=".PersonalTrainingsActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ScheduleBookingActivity" />
        <activity android:name=".StartActivity" />
        <activity android:name=".TrainerActivity" />
        <activity android:name=".UpcomingTrainingsActivity" />

    </application>
</manifest>